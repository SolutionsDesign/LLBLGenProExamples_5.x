<?xml version="1.0"?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5"/>
    </startup>
	<appSettings>
		<!-- alter the connection string below to connect to your server -->
		<add key="ConnectionString.SQL Server (SqlClient)" value="data source=(local);initial catalog=Northwind;integrated security=SSPI;persist security info=False;packet size=4096"/>
	</appSettings>
	
	<!-- Enable tracing by setting the switches below to a value of 3 (info) or 4 (verbose) and run this app in debug mode -->
	<system.diagnostics>
		<switches>
			<add name="SqlServerDQE" value="0"/>
			<add name="ORMGeneral" value="0"/>
			<add name="ORMStateManagement" value="0"/>
			<add name="ORMPersistenceExecution" value="0"/>
		</switches>
	</system.diagnostics>
</configuration>
